    "directory": "packages/rpc"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Erik Kinnunen",
  "license": "MIT",
  "dependencies": {
    "@msgpack/msgpack": "^2.7.1",
    "@types/ws": "^8.2.2",
    "ws": "^8.4.2"
  }
