{
  "name": "@umbrellajs/machine",
  "version": "0.4.1",
  "type": "module",
  "description": "State machine library",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "./dist/cjs/*",
    "./dist/esm/*",
    "./dist/types/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "prepublish": "run g:prepublish && run g:rollup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erikjuhani/umbrellajs",
    "directory": "packages/machine"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Erik Kinnunen",
  "license": "MIT"
}
