{
  "name": "@umbrellajs/random",
  "version": "0.3.1",
  "type": "module",
  "description": "Random number generator",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "./dist/cjs/*",
    "./dist/esm/*",
    "./dist/types/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "prepublish": "run g:prepublish && run g:rollup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erikjuhani/umbrellajs",
    "directory": "packages/random"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Erik Kinnunen",
  "license": "MIT"
}
